<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="30dp"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnMap"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="地图路线/导航"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/OnClick"/>
    <Button
        android:id="@+id/btnAMapNavi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="高德地图导航"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMap"
        style="@style/OnClick"/>
    <Button
        android:id="@+id/btnAMapRoute"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="高德地图路线规划"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAMapNavi"
        style="@style/OnClick"/>
    <Button
        android:id="@+id/btnBaiduMapNavi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="百度地图导航"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAMapRoute"
        style="@style/OnClick"/>
    <Button
        android:id="@+id/btnBaiduMapRoute"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="百度地图路线规划"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnBaiduMapNavi"
        style="@style/OnClick"/>
    <Button
        android:id="@+id/btnTencentMapRoute"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="腾讯地图路线规划"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnBaiduMapRoute"
        style="@style/OnClick"/>
    <Button
        android:id="@+id/btnGoogleMapNavi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="谷歌地图导航"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnTencentMapRoute"
        style="@style/OnClick"/>

</androidx.constraintlayout.widget.ConstraintLayout>